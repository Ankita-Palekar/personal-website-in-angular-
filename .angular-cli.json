{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "personal-website"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "assets/css/color.css",
        "assets/css/jquery.bxslider.css",
        "assets/css/main.css",
        "assets/css/reset.css",
        "assets/css/responsive.css",
        "assets/css/shortcodes.css",
        "assets/css/theme.css",
        "assets/css/responsive.gs.12col.css",
        "../node_modules/font-awesome/css/font-awesome.min.css"
      ],
      "scripts": [
        "assets/js/jquery.1.9.1.min.js",
        "assets/js/jquery.bxslider.js",
        "assets/js/jquery.carouFredSel-6.2.1.js",
        "assets/js/jquery.easing.1.3.js",
        "assets/js/jquery.fitvids.js",
        "assets/js/jquery.googlemap.js",
        "assets/js/jquery.hoverdir.js",
        "assets/js/jquery.mixitup.js",
        "assets/js/jquery.parallax-1.1.3.js",
        "assets/js/modernizr.custom.97074.js",
        "assets/js/respond.min.js",
        "assets/js/SmoothScroll.js",
        "assets/js/theme.mail.js",
        "assets/js/theme.script.js",
        "assets/js/theme.settings.js",
        "assets/js/boxsizing.htc"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
